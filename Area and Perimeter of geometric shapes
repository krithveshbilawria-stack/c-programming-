#include <stdio.h>

int main() {
    float r, a, l, w;
    const float pi = 3.1416;

    // Input
    scanf("%f", &r);       // radius of circle
    scanf("%f", &a);       // side of square
    scanf("%f %f", &l, &w); // length and width of rectangle

    // Circle calculations
    float circle_area = pi * r * r;
    float circle_perimeter = 2 * pi * r;

    // Square calculations
    float square_area = a * a;
    float square_perimeter = 4 * a;

    // Rectangle calculations
    float rectangle_area = l * w;
    float rectangle_perimeter = 2 * (l + w);

    // Output with 2 decimal places
    printf("Circle: %.2f %.2f\n", circle_area, circle_perimeter);
    printf("Square: %.2f %.2f\n", square_area, square_perimeter);
    printf("Rectangle: %.2f %.2f\n", rectangle_area, rectangle_perimeter);

    return 0;
}

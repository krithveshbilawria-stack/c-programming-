#include <stdio.h>

int main() {
    int totalDays;
    int years, weeks, days;
    scanf("%d%d%d%d",&totalDays,&years,&weeks,&days);
    years = totalDays / 365;  
    totalDays %= 365;
    weeks = totalDays / 7;
    days = totalDays % 7;
    printf("Years: %d\nWeeks: %d\nDays: %d\n", years, weeks, days);
    return 0;
}
